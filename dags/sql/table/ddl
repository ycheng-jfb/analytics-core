CREATE TABLE case_table (
    case_id INT PRIMARY KEY AUTOINCREMENT,
    description TEXT,
    status VARCHAR(50),
    datetime_added TIMESTAMP_NTZ(3),
    datetime_modified TIMESTAMP_NTZ(3)
);


-- 假设 company 表的字段如下（未在SQL文件中找到具体结构）
CREATE TABLE company (
    company_id INT PRIMARY KEY AUTOINCREMENT,
    name VARCHAR(255),
    address VARCHAR(255),
    datetime_added TIMESTAMP_NTZ(3),
    datetime_modified TIMESTAMP_NTZ(3)
);
